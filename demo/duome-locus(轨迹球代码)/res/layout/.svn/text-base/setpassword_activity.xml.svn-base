<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@drawable/bg">
	<com.duome.locus.LocusPassWordView
		android:id="@+id/mLocusPassWordView" android:layout_above="@+id/LinearLayout1"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
	</com.duome.locus.LocusPassWordView>
	<LinearLayout android:layout_alignParentLeft="true"
		android:id="@+id/LinearLayout1" android:layout_alignParentBottom="true"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:orientation="horizontal">
		<TextView android:gravity="center_vertical|center"
			android:layout_height="42dp" android:text="保存" android:id="@+id/tvSave"
			android:layout_width="wrap_content" android:layout_weight="1"
			android:textColor="#000000" android:background="@drawable/border"></TextView>
		<TextView android:gravity="center_vertical|center"
			android:layout_height="42dp" android:text="重置" android:id="@+id/tvReset"
			android:layout_width="wrap_content" android:layout_weight="1"
			android:textColor="#000000" android:background="@drawable/border"></TextView>
	</LinearLayout>
</RelativeLayout>
